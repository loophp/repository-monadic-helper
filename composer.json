{
    "name": "loophp/repository-monadic-helper",
    "description": "Monadic Doctrine repositories helper classes and services.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "doctrine",
        "monad"
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/drupol"
        }
    ],
    "require": {
        "php": ">= 7.4",
        "marcosh/lamphpda": "^1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.12",
        "drupol/php-conventions": "^5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "loophp\\RepositoryMonadicHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\loophp\\RepositoryMonadicHelper\\App\\": "tests/App/",
            "tests\\unit\\loophp\\RepositoryMonadicHelper\\App\\": "tests/unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true
        }
    },
    "scripts": {
        "changelog-unreleased": "auto-changelog -c .auto-changelog -u",
        "changelog-version": "auto-changelog -c .auto-changelog -v",
        "grumphp": "./vendor/bin/grumphp run"
    }
}
